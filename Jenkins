pipeline {
    agent any
    stages {
        stage('Deploy CloudFormation Stack') {
            steps {
                sh '''
                "aws cloudformation deploy \ 
                   --stack-name ecinstance10 \
                   --template-file create_ec2.yml \
                   --capabilities CAPABILITY_NAMED_IAM \ 
                   --tags key=Name,Value=125asp01 key=costid,Value=125asp02
                   --region 'us-east-1' 
                '''   
            } 
        }
    }
}
